host !bastion* *.bom*.amazon.com *.bom*.amazon.com. *.bom? *.bom??
        ProxyCommand ssh bastion-bom.amazon.com nc %h %p# Configuration for CMH
host !bastion* *.cmh*.amazon.com *.cmh*.amazon.com. *.cmh? *.cmh??
        ProxyCommand ssh bastion-cmh.amazon.com nc %h %p# Configuration for LHR
host !bastion* *.lhr*.amazon.com *.lhr*.amazon.com. *.lhr? *.lhr??
        ProxyCommand ssh bastion-lhr.amazon.com nc %h %p# Configuration for YUL
host !bastion* *.yul*.amazon.com *.yul*.amazon.com. *.yul? *.yul??
