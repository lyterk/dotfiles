host !bastion* *.kix*.amazon.com *.kix*.amazon.com. *.kix? *.kix??
        ProxyCommand ssh bastion-kix.amazon.com nc %h %p# Configuration for OSU
host !bastion* *.osu*.amazon.com *.osu*.amazon.com. *.osu? *.osu??
        ProxyCommand ssh bastion-osu.amazon.com nc %h %p# Configuration for ARN
host !bastion* *.arn*.amazon.com *.arn*.amazon.com. *.arn? *.arn??
        ProxyCommand ssh bastion-arn.amazon.com nc %h %p# Configuration for HKG
host !bastion* *.hkg*.amazon.com *.hkg*.amazon.com. *.hkg? *.hkg??
        ProxyCommand ssh bastion-hkg.amazon.com nc %h %p# Configuration for BAH
host !bastion* *.bah*.amazon.com *.bah*.amazon.com. *.bah? *.bah??
        ProxyCommand ssh bastion-bah.amazon.com nc %h %p# Configuration for CPT
host !bastion* *.cpt*.amazon.com *.cpt*.amazon.com. *.cpt? *.cpt??
        ProxyCommand ssh bastion-cpt.amazon.com nc %h %p# End Prod Bastion config
